+    "            metadatas = sig.get_full_metadata()\n",
+    "            targets = {i: [] for i in range(len(metadatas))}\n",
+    "            for i, m in enumerate(metadatas):\n",
+    "                for k, v in m.to_dict().items():\n",
+    "                    if v is not None:\n",
